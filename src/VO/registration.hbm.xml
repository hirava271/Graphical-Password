<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="VO.registrationVO" table="registration_mst">

		<id name="user_id" type="int" column="user_id">
			<generator class="increment" />
		</id>

		<property name="name">
			<column name="user_name" />
		</property>

		<property name="number">
			<column name="number" />
		</property>
		
		<property name="email">
			<column name="email" />
		</property>
		
		<property name="date">
			<column name="date" />
		</property>
		
		<property name="pwd">
			<column name="pwd" />
		</property>
		
		<property name="color">
			<column name="color" />
		</property>
		
	</class>

</hibernate-mapping>
